{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate --prerender",
    "generateDev": "NODE_ENV=development && nuxt generate --prerender",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "http-serve dist -p 3001",
