  },
  "dependencies": {
    "nuxt": "^3.12.3",
    "vue": "latest"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "8",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "^1.77.8"
  }
}